	"version": "0.2.0",
	"configurations": [
		{
			"type": "pwa-node",
			"request": "launch",
			"name": "Run Parse Test",
			"skipFiles": [
				"<node_internals>/**"
			],
			"outputCapture": "std",
            "cwd": "${workspaceFolder}/tests/",
			"program": "${workspaceFolder}/tests/parse.js",
			"args": [
				"> ",
				"/users/lrosenth/OUTPUT/out.json"
			]
		},
		{
			"type": "pwa-node",
			"request": "launch",
			"name": "Run parseFile on media/video.m4s",
			"skipFiles": [
				"<node_internals>/**"
			],
			"outputCapture": "std",
            "cwd": "${workspaceFolder}",
			"program": "${workspaceFolder}/parseFile.js",
			"args": [
				"./tests/media/video.m4s"
			]
		},
		{
			"type": "node",
			"request": "launch",
			"name": "Parse MSFT manifest",
			"skipFiles": [
				"<node_internals>/**"
			],
			"outputCapture": "std",
			"program": "${workspaceFolder}/parseFile.js",
			"args": [
				"/Users/lrosenth/INPUT/images/msft.manifest"
			]
		},
		{
			"type": "node",
			"request": "launch",
			"name": "Parse Adobe manifest",
			"skipFiles": [
				"<node_internals>/**"
			],
			"outputCapture": "std",
			"program": "${workspaceFolder}/parseFile.js",
			"args": [
				"/Users/lrosenth/INPUT/images/pngpalm-CAI.jumbf"
			]
		},
		{
			"type": "node",
			"request": "launch",
			"name": "Parse sample MP4",
			"skipFiles": [
				"<node_internals>/**"
			],
			"outputCapture": "std",
			"program": "${workspaceFolder}/parseFile.js",
			"args": [
				"/Users/lrosenth/INPUT/images/sample-mp4-file.mp4"
			]
		}
	]
}